{
  "comments": {
    "lineComment": "COMMENT"
  },
  "brackets": [
    ["BEGIN", "END"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"" }
  ],
  "surroundingPairs": [
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "wordPattern": "[a-zA-Z][a-zA-Z0-9]*",
  "indentationRules": {
    "increaseIndentPattern": "^.*\\b(BEGIN|THEN|DO|ELSE)\\s*$",
    "decreaseIndentPattern": "^\\s*END\\b"
  }
}
